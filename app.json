{
  "expo": {
    "name": "Expense Wallet",
    "slug": "expense-wallet",
    "version": "1.0.0",
    "description": "Track your expenses and income with a simple wallet app. Add transactions and view your financial dashboard.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expensewallet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "versionCode": 1,
      "package": "com.methreudaykumar.expensewallet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38035c31-70a5-4be2-bd6c-6bba5e81e806"
      }
    }
  }
}
